@page
@using Microsoft.EntityFrameworkCore
@model ServiceHost.Pages.ActiveUserModel
@{
}

@if (Model.ErrorMessageame != null)
{
    <div class="alert alert-danger">
        @Model.ErrorMessageame

    </div>
}
<div class="breadcrumb-area section-space--half">
    <div class="container wide">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb-wrapper breadcrumb-bg">
                    <div class="breadcrumb-content">
                        <h2 class="breadcrumb-content__title">تایید تلفن همراه</h2>
                        <ul class="breadcrumb-content__page-map">
                            <li>
                                <a asp-page="./Index">صفحه اصلی</a>
                            </li>
                            <li class="active">تایید تلفن همراه</li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<form method="post" asp-page-handler="Confirm">
    <div class="modal-body">
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label asp-for="@Model.Command.Code" class="control-label">لطفا کد ارسالی را وارد نمایید</label>
                    <input type="text" class="form-control" asp-for="@Model.Command.Code">
                    <span asp-validation-for="@Model.Command.Code" class="error"></span>
                </div>
            </div>


        </div>
    </div>
    <div class="modal-footer">

        <button type="submit" class="btn btn-info waves-effect waves-light">تایید</button>
    </div>
</form>